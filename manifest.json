{
  "manifest_version": 3,
  "name": "Panel",
  "short_name": "AdminPanel",
  "description": "Panel",
  "version": "1.0.0",
  "icons": {
    "16": "favicon.ico"
  },
  "theme_color": "#4a90e2",
  "background_color": "#4a90e2",
  "start_url": "/",
  "display": "minimal-ui",
  "permissions": [
    "storage",
    "notifications"
  ],
  "action": {
    "default_popup": "",
    "default_icon": {
      "16": "favicon.ico"
    }
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
